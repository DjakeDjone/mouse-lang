fn test_func(x) {
    if x > 5 {
        return 10;
    }
    return 5;
}

fn another_func(a, b) {
    let sum = a + b;
    if sum > 10 {
        return sum * 2;
    }
    return sum;
}

let result1 = test_func(3);
let result2 = test_func(7);
let result3 = another_func(5, 8);
let result4 = another_func(2, 3);

fn nested_calls() {
    let x = test_func(6);
    let y = another_func(x, 5);
    return y;
}

let result5 = nested_calls();
